<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string><%= @boxen_user %>.mysqldump-all</string>
        <key>Program</key>
        <string><%= @boxen_srcdir %>/mysql/bin/mysqldump-all.sh</string>
        <key>StandardErrorPath</key>
        <string>/tmp/<%= @boxen_user %>.mysqldump-all.stderr</string>
        <key>StandardOutPath</key>
        <string>/tmp/<%= @boxen_user %>.mysqldump-all.stdout</string>
        <key>StartCalendarInterval</key>
        <dict>
                <key>Hour</key>
                <integer>0</integer>
                <key>Minute</key>
                <integer>0</integer>
                <key>Weekday</key>
                <integer>3</integer>
        </dict>
</dict>
</plist>